<form class="form-horizontal">
    <div class="information">The print queue is sorted on print type, then submission date. Put your highest-priority type at the top of this list and your lowest priority at the bottom.</div>
    <div class="warning"  data-bind="ifnot:queueIsEmpty" style="margin-top:15px;margin-bottom:15px;">
        <strong>WARNING:</strong>The print queue is not empty. Changes made to the available print types will not change the current order of the items in the queue.
    </div>

    <div class="row-fluid">
        <div class="span12"><h4>Submission type</h4></div>
    </div>

    <div data-bind="foreach: {data: printtypes, as: 'type'}">
        <div class="row-fluid" style="margin-bottom: 5px">
            <div class="span9">
                <input class="printTypeEntry span12" data-bind="textInput: type">
            </div>
            <div class="span1">
                <a title="Move up" class="btn btn-up" data-bind="click: $parent.movePrintTypeUp"><i class="fa fa-arrow-up"></i></a>
            </div>
            <div class="span1">
                <a title="Move down" class="btn btn-down" data-bind="click: $parent.movePrintTypeDown"><i class="fa fa-arrow-down"></i></a>
            </div>
            <div class="span1">
                <a title="Delete" class="btn btn-danger" data-bind="click: $parent.removePrintType"><i class="icon-trash"></i></a>
            </div>
        </div>
    </div>

    <div class="row-fluid">
        <div class="offset10 span2">
            <a title="Add type" class="btn btn-primary" data-bind="click: addNewPrintType"><i class="icon-plus"></i></a>
        </div>
    </div>
</form>
